		<func id="getComboData" name="获得控件枚举列表" desc="获取下拉框绑定的枚举列表">
			<demo>
				<![CDATA[
ComboBoxComp ${eleId}_comp = (ComboBoxComp)LuiAppUtil.getControl("${eleId}");
DataList ${eleId}_dl = (DataList) ${eleId}_comp.getComboData();
DataItem[] ${eleId}_dl_items = ${eleId}_dl.getAllDataItems();
			  		]]>
			</demo>
		</func>
		<func id="addItem" name="添加枚举项" desc="向控件的枚举列表中添加一项">
			<demo>
				<![CDATA[
ComboBoxComp ${eleId}_comp = (ComboBoxComp)LuiAppUtil.getControl("${eleId}");
DataList ${eleId}_dl = (DataList) ${eleId}_comp.getComboData();
DataItem newItem = new DataItem();
newItem.setValue("");//真实值
newItem.setText("");//显示值
${eleId}_dl.addDataItem(newItem);
			  		]]>
			</demo>
		</func>
		<func id="addItem" name="移除枚举项" desc="在控件的枚举列表中移除一项">
			<demo>
				<![CDATA[
ComboBoxComp ${eleId}_comp = (ComboBoxComp)LuiAppUtil.getControl("${eleId}");
DataList ${eleId}_dl = (DataList) ${eleId}_comp.getComboData();
${eleId}_dl.removeDataItem(DataItem/DataItemId);
			  		]]>
			</demo>
		</func>
