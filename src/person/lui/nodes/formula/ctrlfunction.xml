			  		]]>
			</demo>
		</func>
		<func id="getValue" name="获得元素值" desc="双击即可生成获取该元素值的代码块">
			<demo>
				<![CDATA[
FormComp ${pEleId}_form = (FormComp)LuiAppUtil.getControl("${pEleId}");
FormElement ${eleId}_formele = ${pEleId}_form.getElementById("${eleId}");
String ${pEleId}_dsId = ${pEleId}_form.getDataset();
Dataset ${pEleId}_ds = LuiAppUtil.getDataset(${pEleId}_dsId);
Row ${pEleId}_ds_selRow = ${pEleId}_ds.getSelectedRow();
if(${pEleId}_ds_selRow == null)
	return "";
String value = ${pEleId}_ds_selRow.getString(${pEleId}_ds.nameToIndex(${eleId}_formele.getField()));
			  		]]>
			</demo>
		</func>
	</ctrltype>
