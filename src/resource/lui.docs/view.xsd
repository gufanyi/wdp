<?xml version="1.0" encoding="UTF-8"?>
<xs:schema   xmlns:xs="http://www.w3.org/2001/XMLSchema" 
targetNamespace="http://com.founer.xap/schema/lui"  xmlns="http://com.founer.xap/schema/lui" elementFormDefault="qualified">
  <xs:element name="ViewPart">
    <xs:complexType>
      <xs:all>
        <xs:element minOccurs="0" maxOccurs="1" ref="PipeOuts"/>
        <xs:element minOccurs="0" maxOccurs="1" ref="PipeIns"/>
        <xs:element minOccurs="0" maxOccurs="1" ref="DataModels"/>
        <xs:element minOccurs="0" maxOccurs="1" ref="Controls"/>
        <xs:element minOccurs="0" maxOccurs="1" ref="Menus"/>
        <xs:element minOccurs="0" maxOccurs="1" ref="Events"/>
      </xs:all>
      <xs:attribute name="controller" use="required" type="xs:string" default="控制类"/>
      <xs:attribute name="id" use="required" type="xs:string" default="main"/>
      <xs:attribute name="isDialog" use="required" type="xs:boolean" default="true"/>
      <xs:attribute name="refId" type="xs:NCName" default="main"/>
      <xs:attribute name="srcFolder" type="xs:string" default="src/public/"/>
      <xs:attribute name="isVisible" type="xs:string" default="true"/>
      <xs:attribute name="enabled" type="xs:string" default="true"/>
      <xs:attribute name="isCustom" type="xs:string" default="true"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="PipeOuts">
    <xs:complexType/>
  </xs:element>
  <xs:element name="PipeIns">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="PipeIn"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="PipeIn">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="PipeInItem"/>
      </xs:sequence>
      <xs:attribute name="id" use="required" type="xs:string" default="refOkPlugin"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="PipeInItem">
    <xs:complexType>
      <xs:attribute name="id" use="required" type="carType" default="type">
        <xs:simpleType name="carType">
          <xs:restriction base="xs:string">
            <xs:enumeration value="type"/>
            <xs:enumeration value="id"/>
            <xs:enumeration value="writeFields"/>
            <xs:enumeration value="row"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="type"  type="xs:NCName" default="java.lang.String"/>
      <xs:attribute name="clazztype"  type="xs:NCName" default="java.lang.String"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="DataModels">
    <xs:complexType>
      <xs:choice maxOccurs="unbounded">
        <xs:element minOccurs="0" maxOccurs="1" ref="DatasetRelations"/>
        <xs:element minOccurs="0" maxOccurs="1" ref="MdDataset"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="Dataset"/>
        <xs:element minOccurs="0" maxOccurs="1" ref="RefNodes"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="DataList"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="RefMdDataset"/>
      </xs:choice>
    </xs:complexType>
  </xs:element>
    <xs:element name="DatasetRelations">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="DatasetRelation"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
    <xs:element name="DatasetRelation">
    <xs:complexType>
      <xs:attribute name="id" use="required" type="xs:NCName"/>
      <xs:attribute name="masterDataset" use="required" type="xs:NCName"/>
      <xs:attribute name="masterKeyField" use="required" type="xs:NCName" />
      <xs:attribute name="detailDataset" use="required" type="xs:NCName" />
      <xs:attribute name="detailForeignKey" use="required" type="xs:NCName" />
    </xs:complexType>
  </xs:element>
  <xs:element name="MdDataset">
    <xs:complexType>
      <xs:choice maxOccurs="unbounded">
        <xs:element minOccurs="0" ref="Fields"/>
        <xs:element minOccurs="0" ref="Events"/>
        <xs:element minOccurs="0" ref="FieldRelations"/>
      </xs:choice>
      <xs:attribute name="caption" use="required" type="xs:NCName"/>
      <xs:attribute name="id" use="required" type="xs:string"/>
      <xs:attribute name="isEdit" use="required" type="xs:boolean" default="true"/>
      <xs:attribute name="isLazyLoad" use="required" type="xs:boolean" default="false"/>
      <xs:attribute name="pageSize" use="required" type="xs:integer" default="-1"/>
      <xs:attribute name="focusIndex" type="xs:integer" default="-1"/>
      <xs:attribute name="voMeta" use="required" type="xs:NCName" default="全类名"/>
      <xs:attribute name="tableName" type="xs:NCName" />
      <xs:attribute name="isCleared" type="xs:NCName" />
      <xs:attribute name="objMeta" type="xs:NCName" />
    </xs:complexType>
  </xs:element>
  <xs:element name="Dataset">
    <xs:complexType>
      <xs:choice maxOccurs="unbounded">
        <xs:element minOccurs="0" maxOccurs="1" ref="Fields"/>
        <xs:element minOccurs="0" maxOccurs="1" ref="Events"/>
        <xs:element minOccurs="0" ref="FieldRelations"/>
      </xs:choice>
      <xs:attribute name="caption" use="required" type="xs:NCName"/>
      <xs:attribute name="id" use="required" type="xs:string"/>
      <xs:attribute name="isEdit" use="required" type="xs:boolean" default="true"/>
      <xs:attribute name="isLazyLoad" use="required" type="xs:boolean" default="false"/>
      <xs:attribute name="pageSize" use="required" type="xs:integer" default="-1"/>
      <xs:attribute name="focusIndex" type="xs:integer" default="-1"/>
      <xs:attribute name="voMeta" type="xs:string"/>
      <xs:attribute name="isCleared" type="xs:boolean" default="false"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="Fields">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="Field"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="Field">
    <xs:complexType>
      <xs:attribute name="dataType"  type="xs:NCName" default="String"/>
      <xs:attribute name="id" use="required" type="xs:string"/>
      <xs:attribute name="isPK" use="required" type="xs:boolean" default="false"/>
      <xs:attribute name="isRequire" use="required" type="xs:boolean" default="true"/>
      <xs:attribute name="text" use="required" type="xs:NCName" default="文本"/>
      <xs:attribute name="field" type="xs:NCName" />
      <xs:attribute name="isLock" type="xs:boolean" default="false"/>
      <xs:attribute name="ctxChanged" type="xs:boolean" default="false"/>
      <xs:attribute name="text" use="required" type="xs:NCName" default="文本"/>
      <xs:attribute name="text" use="required" type="xs:NCName" default="文本"/>
      <xs:attribute name="text" use="required" type="xs:NCName" default="文本"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="RefNodes">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="GenericRefNode"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="GenericRefNode">
    <xs:complexType>
      <xs:attribute name="controller" use="required" type="xs:string" default="节点控制类"/>
      <xs:attribute name="height" use="required" type="nonNegativeInteger" default="500"/>
      <xs:attribute name="id" use="required" type="xs:string"/>
      <xs:attribute name="isMultiple" use="required" type="xs:boolean" default="false"/>
      <xs:attribute name="isOnlyLeaf" use="required" type="xs:boolean" default="false"/>
      <xs:attribute name="isQuickInput" use="required" type="xs:boolean" default="false"/>
      <xs:attribute name="readDataset" use="required" type="xs:NCName" default="读取的数据集"/>
      <xs:attribute name="readFields" use="required" tape="xs:string" default="读数据集的字段"/>
      <xs:attribute name="refcode" use="required" type="xs:NCName" default="参照的编码"/>
      <xs:attribute name="title" use="required" type="xs:string" default="参照弹出框的标题"/>
      <xs:attribute name="isLocate" use="required" type="xs:boolean" default="true"/>
      <xs:attribute name="isRead" use="required" type="xs:boolean" default="true"/>
      <xs:attribute name="isFilterNames" use="required" type="xs:boolean" default="false"/>
      <xs:attribute name="pageMeta" use="required" type="xs:NCName" default="reference"/>
      <xs:attribute name="width" use="required" type="xs:nonNegativeInteger" default="600"/>
      <xs:attribute name="winModel" type="xs:string"/>
      <xs:attribute name="writeDataset" use="required" type="xs:string"/>
      <xs:attribute name="writeFields" use="required" type="xs:string"/>
      <xs:attribute name="filterSql"  type="xs:string"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="DataList">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="DataItem"/>
      </xs:sequence>
      <xs:attribute name="caption" type="xs:string"/>
      <xs:attribute name="id" use="required" type="xs:NCName"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="DataItem">
    <xs:complexType>
      <xs:attribute name="text" use="required" type="xs:string" default="显示值"/>
      <xs:attribute name="value" use="required" type="xs:string" default="条目真实值"/>
    </xs:complexType>
  </xs:element>
   <xs:element name="RefMdDataset">
    <xs:complexType>
      <xs:choice maxOccurs="unbounded">
        <xs:element minOccurs="0" ref="Fields"/>
        <xs:element minOccurs="0" ref="FieldRelations"/>
      </xs:choice>
      <xs:attribute name="caption" type="xs:NCName" />
      <xs:attribute name="focusIndex" use="required" type="xs:integer" default="-1"/>
      <xs:attribute name="id" use="required" type="xs:string" />
      <xs:attribute name="isCleared" use="required" type="xs:boolean" default="false"/>
      <xs:attribute name="isEdit" use="required" type="xs:boolean" default="true"/>
      <xs:attribute name="tableName" use="required" type="xs:string" />
      <xs:attribute name="isLazyLoad" use="required" type="xs:boolean" default="false"/>
      <xs:attribute name="pageSize" use="required" type="xs:integer" default="-1"/>
      <xs:attribute name="voMeta" use="required" type="xs:NCName" default="全类名"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="Controls">
    <xs:complexType>
      <xs:choice maxOccurs="unbounded">
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="CheckBox"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="CheckBoxGroup"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="ComboBox"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="Text"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="StringText"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="IntegerText"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="DecimalText"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="PwdText"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="TextArea"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="Radio"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="RadioGroup"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="DateText"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="Reference"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="Label"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="Link"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="Iframe"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="Form"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="Tree"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="Button"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="Grid"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="ToolBar"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="HtmlContent"/>
      </xs:choice>
    </xs:complexType>
  </xs:element>
   <xs:element name="CheckBox">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="1" ref="Events"/>
      </xs:sequence>
      <xs:attribute name="checked" use="required" type="xs:boolean" default="false"/>
      <xs:attribute name="editorType" use="required" type="xs:NCName" default="CheckBox"/>
      <xs:attribute name="id" use="required" type="xs:string"/>
      <xs:attribute name="isVisible" use="required" type="xs:boolean" default="true"/>
      <xs:attribute name="text" use="required" type="xs:NCName" default="checkbox旁边显示的文字"/>
      <xs:attribute name="value" use="required" type="xs:NCName" default="checkbox显示值"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="CheckBoxGroup">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="1" ref="Events"/>
      </xs:sequence>
      <xs:attribute name="value" use="required" type="xs:String" />
      <xs:attribute name="readOnly" use="required" type="xs:boolean" default="false"/>
      <xs:attribute name="changeLine" use="required" type="xs:boolean" default="false"/>
      <xs:attribute name="isVisible" use="required" type="xs:boolean" default="true"/>
      <xs:attribute name="dataListId" use="required" type="xs:NCName" default="对应的DataList的id"/>
      <xs:attribute name="id" use="required" type="xs:string" />
      <xs:attribute name="refData" use="required" type="xs:NCName" />
      <xs:attribute name="sepWidth" use="required" type="xs:integer" default="1"/>
      <xs:attribute name="text" use="required" type="xs:NCName" />
      <xs:attribute name="tabIndex" use="required" type="xs:NCName" />
    </xs:complexType>
  </xs:element>
  <xs:element name="ComboBox">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="1" ref="Events"/>
      </xs:sequence>
      <xs:attribute name="align" use="required" type="xs:NCName" default="left"/>
      <xs:attribute name="enabled" use="required" type="xs:boolean" default="true"/>
      <xs:attribute name="focus" use="required" type="xs:boolean" default="false"/>
      <xs:attribute name="id" use="required" type="xs:string"/>
      <xs:attribute name="isAllowExtendValue" use="required" type="xs:boolean" default="true"/>
      <xs:attribute name="isOnlySelect" use="required" type="xs:boolean" default="true"/>
      <xs:attribute name="isVisible" use="required" type="xs:boolean" default="true"/>
      <xs:attribute name="refComboData" use="required" type="xs:NCName"/>
      <xs:attribute name="text" use="required" type="xs:NCName" default="ComboBox旁边显示的文字"/>
      <xs:attribute name="textWidth" use="required" type="xs:integer" default="0"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="StringText">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="1" ref="Events"/>
      </xs:sequence>
      <xs:attribute name="enabled" use="required" type="xs:boolean" default="true"/>
      <xs:attribute name="id" use="required" type="xs:string"/>
      <xs:attribute name="isVisible" use="required" type="xs:boolean" default="true"/>
      <xs:attribute name="readOnly" use="required" type="xs:boolean" default="false"/>
      <xs:attribute name="text" use="required" type="xs:string" default="StringText旁边显示的文字"/>
      <xs:attribute name="value" use="required" type="xs:string"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="Text">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="1" ref="Events"/>
      </xs:sequence>
      <xs:attribute name="enabled" use="required" type="xs:boolean" default="true"/>
      <xs:attribute name="id" use="required" type="xs:string"/>
      <xs:attribute name="isVisible" use="required" type="xs:boolean" default="true"/>
      <xs:attribute name="readOnly" use="required" type="xs:boolean" default="false"/>
      <xs:attribute name="text" use="required" type="xs:string" default="StringText旁边显示的文字"/>
      <xs:attribute name="value" use="required" type="xs:string"/>
      <xs:attribute name="editorType" use="required" type="xs:string" default="StringText"/>
      <xs:attribute name="focus" use="required" type="xs:boolean" default="true"/>
      <xs:attribute name="align" use="required" type="xs:string" default="left"/>
      <xs:attribute name="textWidth" use="required" type="xs:integer" default="0"/>
      <xs:attribute name="showMark" use="required" type="xs:boolean" default="false"/>
      <xs:attribute name="isRequired" use="required" type="xs:boolean" default="true"/>
      <xs:attribute name="isShowlabel" use="required" type="xs:boolean" default="true"/>
    </xs:complexType>
  </xs:element>
   <xs:element name="IntegerText">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="1" ref="Events"/>
      </xs:sequence>
      <xs:attribute name="focus" use="required" type="xs:boolean" default="true"/>
      <xs:attribute name="id" use="required" type="xs:string"/>
      <xs:attribute name="isVisible" use="required" type="xs:boolean" default="true"/>
      <xs:attribute name="text" use="required" type="xs:NCName" default="IntegerText旁边显示的文字"/>
    </xs:complexType>
  </xs:element>
   <xs:element name="DecimalText">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="1" ref="Events"/>
      </xs:sequence>
      <xs:attribute name="focus" use="required" type="xs:boolean" default="true"/>
      <xs:attribute name="id" use="required" type="xs:string"/>
      <xs:attribute name="isVisible" use="required" type="xs:boolean" default="true"/>
      <xs:attribute name="text" use="required" type="xs:NCName" default="DecimalText旁边显示的文字"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="PwdText">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="1" ref="Events"/>
      </xs:sequence>
      <xs:attribute name="focus" use="required" type="xs:boolean" default="true"/>
      <xs:attribute name="id" use="required" type="xs:string"/>
      <xs:attribute name="isVisible" use="required" type="xs:boolean" default="true"/>
      <xs:attribute name="text" use="required" type="xs:NCName" default="PwdText旁边显示的文字"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="TextArea">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="1" ref="Events"/>
      </xs:sequence>
      <xs:attribute name="cols" use="required" type="xs:integer" default="400"/>
      <xs:attribute name="focus" use="required" type="xs:boolean" default="true"/>
      <xs:attribute name="id" use="required" type="xs:string"/>
      <xs:attribute name="isVisible" use="required" type="xs:boolean" default="true"/>
      <xs:attribute name="rows" use="required" type="xs:integer" default="3"/>
      <xs:attribute name="text" use="required" type="xs:NCName" default="TextArea旁边显示的文字"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="Radio">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="1" ref="Events"/>
      </xs:sequence>
      <xs:attribute name="checked" use="required" type="xs:boolean" default="false"/>
      <xs:attribute name="group" use="required" type="xs:NCName"/>
      <xs:attribute name="id" use="required" type="xs:string"/>
      <xs:attribute name="position" use="required"/>
      <xs:attribute name="text" use="required" type="xs:NCName" default="Radio边显示的文字"/>
      <xs:attribute name="value" use="required" type="xs:NCName" default="真实值"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="RadioGroup">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="1" ref="Events"/>
      </xs:sequence>
      <xs:attribute name="changeLine" use="required" type="xs:boolean" default="false"/>
      <xs:attribute name="dataListId" use="required" type="xs:NCName" default="Datalist的Id"/>
      <xs:attribute name="id" use="required" type="xs:string"/>
      <xs:attribute name="refData" use="required" type="xs:NCName" />
      <xs:attribute name="sepWidth" use="required" type="xs:integer" default="1"/>
      <xs:attribute name="text" use="required" type="xs:string" default="RadioGroup边显示的文字"/>
      <xs:attribute name="index" use="required" type="xs:integer" default="-1"/>
      <xs:attribute name="tabIndex" use="required" type="xs:integer" default="0"/>
      <xs:attribute name="value" use="required" type="xs:string"/>
      <xs:attribute name="readOnly" use="required" type="xs:boolean" default="false"/>
      <xs:attribute name="editorType" use="required" type="xs:NCName" default="StringText"/>
      <xs:attribute name="focus" use="required" type="xs:boolean" default="false"/>
      <xs:attribute name="align" use="required" type="xs:string" default="left"/>
      <xs:attribute name="textWidth" use="required" type="xs:integer" default="0"/>
      <xs:attribute name="showMark" use="required" type="xs:boolean" default="false"/>
      <xs:attribute name="isRequired" use="required" type="xs:boolean" default="false"/>
      <xs:attribute name="isShowlabel" use="required" type="xs:boolean" default="true"/>
      <xs:attribute name="isVisible" use="required" type="xs:boolean" default="true"/>
      <xs:attribute name="enabled" use="required" type="xs:boolean" default="true"/>
      <xs:attribute name="isRequired" use="required" type="xs:boolean" default="false"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="DateText">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="1" ref="Events"/>
      </xs:sequence>
      <xs:attribute name="align" use="required" type="xs:NCName" default="left"/>
      <xs:attribute name="enabled" use="required" type="xs:boolean" default="true"/>
      <xs:attribute name="focus" use="required" type="xs:boolean"/>
      <xs:attribute name="id" use="required" type="xs:string"/>
      <xs:attribute name="isVisible" use="required" type="xs:boolean" default="true"/>
      <xs:attribute name="text" use="required" type="xs:NCName" default="DateText边显示的文字"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="Reference">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="1" ref="Events"/>
      </xs:sequence>
      <xs:attribute name="align" use="required" type="xs:NCName" default="left"/>
      <xs:attribute name="enabled" use="required" type="xs:boolean" default="true"/>
      <xs:attribute name="focus" use="required" type="xs:boolean" default="false"/>
      <xs:attribute name="id" use="required" type="xs:string"/>
      <xs:attribute name="isVisible" use="required" type="xs:boolean" default="true"/>
      <xs:attribute name="refcode" use="required" type="xs:NCName" default="参照的编码"/>
      <xs:attribute name="text" use="required" type="xs:NCName" default="Reference边显示的文字"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="Label">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="1" ref="Events"/>
      </xs:sequence>
      <xs:attribute name="id" use="required" type="xs:string"/>
      <xs:attribute name="innerHTML" type="xs:string" />
      <xs:attribute name="text" use="required"  type="xs:string" default="文本"/>
      <xs:attribute name="isVisible" use="required" type="xs:boolean" default="true"/>
      <xs:attribute name="enabled" use="required" type="xs:boolean" default="true"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="Link">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="1" ref="Events"/>
      </xs:sequence>
      <xs:attribute name="href" use="required" type="xs:NCName" default="连接路径url"/>
      <xs:attribute name="id" use="required" type="xs:string"/>
      <xs:attribute name="target" use="required" type="carType" default="_blank">
